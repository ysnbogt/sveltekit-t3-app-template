{
  "description": "",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "vercel-build": "prisma generate && prisma migrate deploy && vite build",
    "lint": "eslint 'src/**/*.{js,ts,svelte}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,svelte}'",
    "format": "npm run lint:fix && prettier --write 'src/**/*.{js,ts,svelte}'",
    "test": "vitest",
    "test:playwright": "playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "^1.5",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "prisma": "^4.13.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.0",
    "vite": "^4.0.1",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@auth/core": "^0.7.1",
    "@auth/sveltekit": "^0.3.1",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.13.0",
    "@trpc/client": "^10.23.0",
    "@trpc/server": "^10.23.0",
    "next-auth": "^4.22.1",
    "trpc-sveltekit": "^3.5.6",
    "zod": "^3.21.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run format"
  },
  "type": "module"
}
